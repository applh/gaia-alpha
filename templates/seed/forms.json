        "schema": [
            {
                "id": "name",
                "type": "text",
                "label": "Full Name",
                "required": true,
                "placeholder": "Jane Doe"
            },
            {
                "id": "email",
                "type": "email",
                "label": "Email Address",
                "required": true,
                "placeholder": "jane@company.com"
            },
            {
                "id": "subject",
                "type": "select",
                "label": "Subject",
