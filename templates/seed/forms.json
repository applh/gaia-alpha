                "options": [
                    "General Inquiry",
                    "Sales",
                    "Support",
                    "Partnerships"
                ]
            },
            {
                "id": "message",
                "type": "textarea",
                "label": "Message",
                "required": true,
                "rows": 5,
                "placeholder": "How can we help you?"
            }
        ],
        "submissions": [
            {
                "name": "Initial Client",
                "email": "client@example.com",
                "subject": "Sales",
                "message": "We are interested in your enterprise solutions."
            }
        ]
    },
    {
        "title": "Request a Quote",
        "slug": "quote",
        "description": "Tell us about your project needs.",
        "submit_label": "Get Quote",
        "schema": [
            {
                "id": "company",
                "type": "text",
                "label": "Company Name",
                "required": true
            },
            {
                "id": "service_type",
                "type": "select",
                "label": "Service Needed",
